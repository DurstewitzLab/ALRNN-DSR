    "scalar_saving_interval": 25,
    "image_saving_interval": 5000,
    "gaussian_noise_level": 0.00,  

    "D_stsp_bins": 8,
    "D_stsp_scaling": 1.0,
    "PSE_smoothing": 1.0,
    "PE_n": 20
