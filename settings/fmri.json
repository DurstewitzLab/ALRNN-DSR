{   
    "experiment": "default",
    "name": "test",
    "path_to_data": "example_data/fmri.npy",
    "run": 1,
    "device": "cpu",

    "model": "ALRNN",
    "observation_model": "Identity",
    "latent_dim": 50,
    "pwl_units": 2,
    "optional_model_args": [],

